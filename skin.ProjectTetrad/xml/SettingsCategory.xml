<?xml version="1.0" encoding="utf-8"?>
<window id="12">
    <defaultcontrol>3</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <controls>
        <include condition="!Skin.HasSetting(DontBlendBackground)">CommonVideoVizBackground</include>
        <control type="group">
            <visible>!Window.Previous(Weather)</visible>
            <include>CommonSystemBackground</include>
        </control>
        <control type="group">
            <visible>Window.Previous(Weather)</visible>
            <include>CommonWeatherBackground</include>
        </control>
        <include condition="Skin.HasSetting(DontBlendBackground)">CommonVideoVizBackground</include>
        <control type="group">
            <visible>!Window.Previous(Weather) + !Window.Previous(Videos) + !Window.Previous(MusicFiles) + !Window.Previous(MusicLibrary) + !Window.Previous(Pictures) + !Window.Previous(Programs)</visible>
            <include>CommonSystemBlackwash</include>
        </control>
        <control type="group">
            <visible>Window.Previous(Weather)</visible>
            <include>CommonWeatherBlackwash</include>
        </control>
        <control type="group">
            <visible>Window.Previous(Programs)</visible>
            <include>CommonAppsBlackwash</include>
        </control>
        <control type="group">
            <visible>Window.Previous(Videos) | Window.Previous(MusicFiles) | Window.Previous(MusicLibrary) | Window.Previous(Pictures)</visible>
            <include>CommonMediaBlackwash</include>
        </control>
        <control type="group">
            <posx>195</posx>
            <animation effect="fade" delay="500" time="HomeVisibleFadeAnimationSpeed">WindowOpen</animation>
            <animation effect="fade" start="0" end="0" time="0">WindowClose</animation>
            <control type="label">
                <description>Header label</description>
                <posx>0</posx>
                <label>$LOCALIZE[5] - $INFO[Control.GetLabel(2)]</label>
                <include>HeaderTextCommons</include>
            </control>
            <control type="grouplist" id="3">
                <description>button area</description>
                <posx>0</posx>
                <posy>172</posy>
                <width>375</width>
                <height>750</height>
                <itemgap>-3</itemgap>
                <onleft>5</onleft>
                <onright>5</onright>
                <onup>3</onup>
                <ondown>3</ondown>
            </control>
            <control type="grouplist" id="5">
                <description>control area</description>
                <posx>405</posx>
                <posy>172</posy>
                <width>1185</width>
                <height>750</height>
                <itemgap>-3</itemgap>
                <pagecontrol>90</pagecontrol>
                <onleft>3</onleft>
                <onright>90</onright>
                <onup>5</onup>
                <ondown>5</ondown>
            </control>
            <control type="spincontrol" id="90">
                <description>Next page button</description>
                <posx>1500</posx>
                <posy>180r</posy>
                <subtype>page</subtype>
                <font>font13</font>
                <onleft>5</onleft>
                <onright>3</onright>
                <ondown>90</ondown>
                <onup>90</onup>
                <textcolor>white2</textcolor>
                <showonepage>false</showonepage>
                <animation effect="slide" start="0,0" end="-150,0" time="200" condition="Player.HasAudio">Conditional</animation>
                <animation effect="slide" start="0,0" end="-150,0" time="200" condition="Player.HasVideo + VideoPlayer.Content(Movies)">Conditional</animation>
                <animation effect="slide" start="0,0" end="-240,0" time="200" condition="Player.HasVideo + !VideoPlayer.Content(Movies)">Conditional</animation>
            </control>
        </control>
        <control type="label" id="2">
            <description>Title label2</description>
            <textcolor>-</textcolor>
            <font>-</font>
        </control>
        <control type="group">
            <visible>!Window.Previous(Weather) + !Window.Previous(Videos) + !Window.Previous(MusicFiles) + !Window.Previous(MusicLibrary) + !Window.Previous(Pictures) + !Window.Previous(Programs)</visible>
            <include>SettingsToSubBlades</include>
        </control>
        <control type="group">
            <visible>Window.Previous(Weather)</visible>
            <include>WeatherToSubBlades</include>
        </control>
        <control type="group">
            <visible>Window.Previous(Programs)</visible>
            <include>AppsToSubBlades</include>
        </control>
        <control type="group">
            <visible>Window.Previous(Videos) | Window.Previous(MusicFiles) | Window.Previous(MusicLibrary) | Window.Previous(Pictures)</visible>
            <include>MediaToSubBlades</include>
        </control>
        <control type="button" id="10">
            <description>Default Category Button</description>
            <posx>0</posx>
            <posy>0</posy>
            <height>75</height>
            <width>375</width>
            <textcolor>white</textcolor>
            <texturefocus border="10">button-focus.png</texturefocus>
            <texturenofocus border="10">button-nofocus.png</texturenofocus>
            <label>-</label>
            <align>left</align>
        </control>
        <control type="button" id="7">
            <description>Default Button</description>
            <posx>0</posx>
            <posy>0</posy>
            <height>68</height>
            <font>font13</font>
            <textcolor>white</textcolor>
            <focusedcolor>black</focusedcolor>
            <textoffsetx>15</textoffsetx>
            <texturefocus border="10">list-focus.png</texturefocus>
            <texturenofocus>-</texturenofocus>
            <pulseonselect>no</pulseonselect>
        </control>
        <control type="radiobutton" id="8">
            <description>Default RadioButton</description>
            <posx>0</posx>
            <posy>0</posy>
            <height>68</height>
            <font>font13</font>
            <textcolor>white</textcolor>
            <focusedcolor>black</focusedcolor>
            <texturefocus border="10">list-focus.png</texturefocus>
            <texturenofocus>-</texturenofocus>
            <textoffsetx>15</textoffsetx>
            <radioposx>1125</radioposx>
            <pulseonselect>no</pulseonselect>
        </control>
        <control type="spincontrolex" id="9">
            <description>Default SpinControlex</description>
            <posx>0</posx>
            <posy>0</posy>
            <height>68</height>
            <texturefocus border="10">list-focus.png</texturefocus>
            <texturenofocus>-</texturenofocus>
            <font>font13</font>
            <textcolor>white</textcolor>
            <focusedcolor>black</focusedcolor>
            <focusedcolor2>black</focusedcolor2>
            <disabledcolor>disabled</disabledcolor>
            <textoffsetx>15</textoffsetx>
            <spinposx>10</spinposx>
            <aligny>center</aligny>
            <reverse>yes</reverse>
            <pulseonselect>no</pulseonselect>
        </control>
        <include>Common_Now_Playing</include>
    </controls>
</window>
