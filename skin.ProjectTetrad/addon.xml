<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="skin.tetrad"
  version="0.0.1"
  name="Tetrad"
  provider-name="CheddarGoblin">
  <requires>
    <import addon="xbmc.gui" version="3.00"/>
  </requires>
  <extension
    point="xbmc.gui.skin"
    defaultthemename="textures.xbt"
    debugging="true"
    effectslowdown="1.0">
    <res width="1280" height="720" aspect="16:9" default="true" folder="720p" />
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">MC360... JX720... Tetrad</summary>
    <description lang="en">This is a port of an old XBMC skin from 2012 bringing it up-to-date with latest stable Kodi. Design based off the original Xbox 360 dashboard and the TeamBlackbolt MC360 skin (most main graphics originally by team blackbolt).</description>
    <platform>all</platform>
  </extension>
</addon>
